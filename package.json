{
  "name": "mybusiness",
  "version": "0.0.0",
  "repository": {
    "directory": "projects/design-system",
    "type": "git",
    "url": "git+https://bitbucket.org/adminvoid/starter-angular7.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "ng build --project=mybusiness",
    "build:ds": "ng build --project=design-system && cp styles/styles.css dist/design-system && cp src/assets/icons.svg dist/design-system",
    "build:css": "tailwindcss -i ./src/styles/styles.css -o ./styles/styles.css",
    "watch:css": "tailwindcss -i ./src/styles/styles.css -o ./styles/styles.css --watch",
    "docs": "compodoc -p tsconfig.json -s",
    "icons:build": "npx --yes @vactorynext/svg-spreact-cli ./icons --optimize true --tidy true > ./src/assets/icons.svg"
  },
  "files": [
    "dist"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "angular-svg-icon": "^7.2.1",
    "clipboard": "^2.0.11",
    "core-js": "^2.6.12",
    "rxjs": "~6.5.4",
    "svgson": "^5.3.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.9",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "^7.2.16",
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@compodoc/compodoc": "^1.1.26",
    "@types/node": "~12.11.1",
    "autoprefixer": "^10.4.17",
    "ng-packagr": "^4.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.0",
    "typescript": "~3.1.6",
    "webpack": "^4.39.2"
  }
}
